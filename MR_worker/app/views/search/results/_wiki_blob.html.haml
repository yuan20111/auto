.blob-result
  .file-holder
    .file-title
      = link_to project_wiki_path(@project, wiki_blob.filename) do
        %i.fa.fa-file
        %strong
          = wiki_blob.filename
    .file-content.code.term
      = render 'shared/file_highlight', blob: wiki_blob, first_line_number: wiki_blob.startline, user_color_scheme_class: 'white'
